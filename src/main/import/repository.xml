<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <jahiademo j:dependencies="bootstrap bootstrap-components default font-awesome grid location news person search"
                 j:installedModules="jahiademo"
                 j:modulePriority="0"
                 j:moduleType="templatesSet"
                 j:title="jahiademo"
                 jcr:mixinTypes="jmix:hasExternalProviderExtension"
                 jcr:primaryType="jnt:module">
                <sources jcr:mixinTypes="jmix:hasExternalProviderExtension jmix:sourceControl"
                  jcr:primaryType="jnt:moduleVersionFolder"
                  j:staticMountPointProviderKey="module-jahiademo-1.0-SNAPSHOT">
                    <src jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:folder">
                        <main jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:folder">
                            <resources jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:folder">
                                <jdnt_investorContactForm jcr:primaryType="jnt:nodeTypeFolder">
                                    <html jcr:primaryType="jnt:templateTypeFolder">
                                        <investorContactForm.jsp jcr:primaryType="jnt:viewFile"/>
                                    </html>
                                </jdnt_investorContactForm>
                                <jnt_languageSwitcher jcr:primaryType="jnt:nodeTypeFolder">
                                    <html jcr:primaryType="jnt:templateTypeFolder">
                                        <languageSwitcher.demo.jsp jcr:primaryType="jnt:viewFile"/>
                                    </html>
                                </jnt_languageSwitcher>
                                <jnt_searchResults jcr:primaryType="jnt:nodeTypeFolder">
                                    <html jcr:primaryType="jnt:templateTypeFolder">
                                        <searchHit.jspf jcr:primaryType="jnt:editableFile"/>
                                    </html>
                                </jnt_searchResults>
                                <META-INF jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:metaInfFolder">
                                    <definitions.cnd jcr:mixinTypes="jmix:sourceControl"
                                         jcr:primaryType="jnt:definitionFile"/>
                                </META-INF>
                            </resources>
                        </main>
                    </src>
                </sources>

            <portlets jcr:primaryType="jnt:portletFolder"/>
                <files jcr:primaryType="jnt:folder">
                    <bootstrap jcr:primaryType="jnt:folder">
                        <css jcr:primaryType="jnt:folder">
                            <bootstrap.css jcr:primaryType="jnt:file">
                                <jcr:content jcr:mimeType="text/css" jcr:primaryType="jnt:resource"/>
                            </bootstrap.css>
                        </css>
                    </bootstrap>
                </files>
            <contents jcr:primaryType="jnt:contentFolder"/>
            <templates jcr:primaryType="jnt:templatesFolder">
                <files jcr:primaryType="jnt:folder">
                    <themes jcr:primaryType="jnt:folder">
                            <default jcr:primaryType="jnt:folder"/>
                    </themes>
                        <logo1-default.png j:height="39"
                                  j:width="103"
                                  jcr:mixinTypes="jmix:image"
                                  jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </logo1-default.png>
                </files>
                <contents jcr:primaryType="jnt:contentFolder"/>
                    <base j:theme="/modules/jahiademo/templates/files/themes/default"
                  j:view="jahiademo"
                  jcr:mixinTypes="jmix:theme"
                  jcr:primaryType="jnt:template">
                        <simple jcr:primaryType="jnt:pageTemplate">
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <bootstrap-container jcr:mixinTypes="jmix:bootstrapFullComponentRowPredefined"
                                     jcr:primaryType="jnt:bootstrapFullComponent"
                                     layout="12"
                                     rowType="predefined"/>
                            </pagecontent>
                        </simple>
                        <navmenu jcr:primaryType="jnt:contentList">
                            <navigation-menu jcr:primaryType="jdnt:navmenu"/>
                        </navmenu>
                        <logo jcr:primaryType="jnt:contentList">
                            <logo jcr:primaryType="jdnt:logo"
                        logo="/modules/jahiademo/templates/files/logo1-default.png"/>
                        </logo>
                        <_x0032_-column j:hiddenTemplate="false" jcr:primaryType="jnt:pageTemplate">
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="2 Column"/>
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <bootstrap-container jcr:mixinTypes="jmix:bootstrapFullComponentRowPredefined"
                                                       jcr:primaryType="jnt:bootstrapFullComponent"
                                                       layout="9_3"
                                                       rowType="predefined"/>
                            </pagecontent>
                        </_x0032_-column>
                        <_x0033_-column j:hiddenTemplate="false" jcr:primaryType="jnt:pageTemplate">
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="3 Column"/>
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <bootstrap-container jcr:mixinTypes="jmix:bootstrapFullComponentRowPredefined"
                                                       jcr:primaryType="jnt:bootstrapFullComponent"
                                                       layout="4_4_4"
                                                       rowType="predefined"/>
                            </pagecontent>
                        </_x0033_-column>
                        <home jcr:primaryType="jnt:pageTemplate">
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="default"/>
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <bootstrap-container jcr:mixinTypes="jmix:bootstrapFullComponentRowPredefined"
                                          jcr:primaryType="jnt:bootstrapFullComponent"
                                          layout="12"
                                          rowType="predefined"/>
                            </pagecontent>
                        </home>
                        <header jcr:primaryType="jnt:contentList">
                            <login jcr:primaryType="jdnt:login"/>
                            <language-switcher j:view="demo"
                                     jcr:mixinTypes="jmix:renderable"
                                     jcr:primaryType="jnt:languageSwitcher"
                                     typeOfDisplay="nameInLocale"/>
                        </header>
                        <languageLoginMini jcr:primaryType="jnt:contentList">
                            <language-switcher j:view="demoMini"
                                     jcr:mixinTypes="jmix:renderable"
                                     jcr:primaryType="jnt:languageSwitcher"
                                     typeOfDisplay="nameCurrentLocale"/>
                            <login j:view="mini"
                         jcr:mixinTypes="jmix:renderable"
                         jcr:primaryType="jdnt:login"/>
                        </languageLoginMini>
                        <languageLogin jcr:primaryType="jnt:contentList">
                            <togglesearchbutton jcr:primaryType="jdnt:toggleSearchButton"/>
                            <login jcr:primaryType="jdnt:login"/>
                            <language-switcher j:view="demo"
                                     jcr:mixinTypes="jmix:renderable"
                                     jcr:primaryType="jnt:languageSwitcher"
                                     typeOfDisplay="nameCurrentLocale"/>
                        </languageLogin>
                        <search jcr:primaryType="jnt:contentList">
                            <simple-search-form j:view="demo"
                                      jcr:mixinTypes="jmix:renderable"
                                      jcr:primaryType="jnt:simpleSearchForm"
                                      result="/modules/jahiademo/home/search-results"/>
                        </search>
                        <toggleNav jcr:primaryType="jnt:contentList">
                            <togglenavbarbutton jcr:primaryType="jdnt:toggleNavBarButton"/>
                        </toggleNav>
                        <modals jcr:primaryType="jnt:contentList">
                            <login j:view="modal"
                         jcr:mixinTypes="jmix:renderable"
                         jcr:primaryType="jdnt:login"/>
                        </modals>
                        <company-content-template j:applyOn="jdnt:company"
                                         j:defaultTemplate="true"
                                         j:hiddenTemplate="true"
                                         j:priority="30"
                     jcr:primaryType="jnt:contentTemplate">
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Company Content Template"/>
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <bootstrap-container jcr:mixinTypes="jmix:bootstrapFullComponentRowPredefined"
                                                       jcr:primaryType="jnt:bootstrapFullComponent"
                                                       layout="9_3"
                                          rowType="predefined">
                                    <bootstrap-container jcr:primaryType="jnt:contentList">
                                        <main-resource-display j:mainResourceView="body" jcr:primaryType="jnt:mainResourceDisplay"/>
                                        <companynews jcr:primaryType="jdnt:companyNews">
                                            <j:translation_en jcr:language="en"
                                                jcr:primaryType="jnt:translation"
                                                jcr:title="Digitall Widgets Co News"/>
                                        </companynews>
                                </bootstrap-container>
                                    <bootstrap-container-side1 jcr:primaryType="jnt:contentList">
                                        <main-resource-display j:mainResourceView="metadata" jcr:primaryType="jnt:mainResourceDisplay"/>
                                        <map-static j:bindedComponent="/modules/jahiademo/templates/base/company-content-template/pagecontent/bootstrap-container/bootstrap-container/main-resource-display"
                                       j:height="255"
                                       j:imageType="png8"
                                       j:mapType="roadmap"
                                       j:markerColor="normal"
                                       j:markerSize="normal"
                                       j:width="255"
                                       j:zoom="auto"
                                       jcr:primaryType="jnt:staticMap"/>
                                        <main-resource-display-1 j:mainResourceView="addPeople"
                                                    jcr:primaryType="jnt:mainResourceDisplay"/>
                                    </bootstrap-container-side1>
                                </bootstrap-container>
                            </pagecontent>
                            <landing jcr:primaryType="jnt:contentList">
                                <main-resource-display j:mainResourceView="banner" jcr:primaryType="jnt:mainResourceDisplay"/>
                            </landing>
                        </company-content-template>
                    </base>
                    <newsletter-issue j:applyOn="jnt:newsletterIssue"
                              j:defaultTemplate="true"
                              j:hiddenTemplate="false"
                              j:priority="20"
                              j:view="jahiademoNewsletter"
                              jcr:primaryType="jnt:contentTemplate"/>
            </templates>
                
                <home j:isHomePage="true"
               j:templateName="home"
               jcr:primaryType="jnt:page">
                    <j:translation_en jcr:language="en"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Home"/>
                    <search-results j:templateName="simple" jcr:primaryType="jnt:page">
                        <j:translation_en jcr:language="en"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="Search Results"/>
                    <pagecontent jcr:primaryType="jnt:contentList">
                            <advanced-search-form jcr:primaryType="jnt:advancedSearchForm"/>
                        <search-results autoSuggest="true"
                               autoSuggestHitCount="2"
                               autoSuggestMaxTermCount="1"
                               autoSuggestMinimumHitCount="2"
                               jcr:primaryType="jnt:searchResults"/>
                    </pagecontent>
                        <landing jcr:primaryType="jnt:contentList">
                            <simple-search-form j:view="banner"
                                      jcr:mixinTypes="jmix:renderable"
                                      jcr:primaryType="jnt:simpleSearchForm"
                                      result="/modules/jahiademo/home/search-results"/>
                        </landing>
                    </search-results>
                </home>
        </jahiademo>
    </modules>
</content>
